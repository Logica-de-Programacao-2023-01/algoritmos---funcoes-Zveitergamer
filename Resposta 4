package main

import "fmt"

func segundoMaiorValor(slice []int) int {
	maior := slice[0]
	segundoMaior := slice[0]

	for _, valor := range slice {
		if valor > maior {
			segundoMaior = maior
			maior = valor
		} else if valor > segundoMaior && valor < maior {
			segundoMaior = valor
		}
	}

	return segundoMaior
}

func main() {
	valores := []int{10, 5, 8, 15, 12}
	segundoMaior := segundoMaiorValor(valores)

	fmt.Println("O segundo maior valor Ã©:", segundoMaior)
}
